def hasLoop(head):
  hashTable = set()

  while head:
    if head in hashTable:
      return True
    else:
      hashTable.add(head)
      head = head.next

  return False

def main():
  head = Node(1)
  head.next = Node(2)
  head.next.next = Node(3)
  head.next.next.next = head

  if hasLoop(head):
    print("The linked list has a loop.")
  else:
    print("The linked list does not have a loop.")

if __name__ == "__main__":
  main()

class Node:

  def __init__(self, data):
    self.data = data
    self.next = None
